{
  "extends": ["airbnb", "prettier", "prettier/prettier"],
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "parserOptions": {
    "ecmaVersion": 2021,
    "sourceType": "module"
  },
  "plugins": ["prettier"],
  "rules": {
    // 프로젝트에 맞는 규칙을 추가/수정
    "prettier/prettier": "error"
  }
}
